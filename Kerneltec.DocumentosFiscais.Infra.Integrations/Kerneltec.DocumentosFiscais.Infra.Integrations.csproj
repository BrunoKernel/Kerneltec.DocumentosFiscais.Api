    </None>
  </ItemGroup>


  <ItemGroup>
    <None Update="Acbr\NFeLib\*.*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>


</Project>
